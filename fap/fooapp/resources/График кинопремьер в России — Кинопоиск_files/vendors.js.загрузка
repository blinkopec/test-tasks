searchtypes['page'] = 'Страницы Кинопоиска';
searchtypes['user'] = 'Пользователи';
searchtypes['keyword'] = 'Ключевые слова';
var mode = '';
var ModeSettingsex = [];
ModeSettingsex[''] = [];
ModeSettingsex['']['handler'] = "handler_search.php";
